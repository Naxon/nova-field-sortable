{
    "name": "naxon/nova-field-sortable",
    "description": "A Laravel Nova field.",
    "keywords": [
        "laravel",
        "nova"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.0",
        "laravel/nova": "*",
        "spatie/eloquent-sortable": "^4.0"
    },
    "autoload": {
        "psr-4": {
            "Naxon\\NovaFieldSortable\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Naxon\\NovaFieldSortable\\FieldServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
